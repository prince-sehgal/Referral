
@{
    ViewBag.Title = "Candidates";
    Layout = "~/Views/shared/_layout.cshtml";
}

<div ng-controller="candidatesCtrl">
    <form id="formCandidates">
        <div>
                <div class="ng-cloak" ng-show="divCandidate==0">
                    <div class="row">
                        <!-- left column -->
                        <div class="col-md-12">
                            <!-- general form elements -->
                            <div class="card card-outline">
                                <div class="card-header bg-gray-light">
                                    <h5 class="mb-0" style="font-weight:bold;">Create  Candidate</h5>
                                    <div style="float:right; margin-top:-24px;">

                                        <button type="button" ng-click="saveCandidates()" class="btn btn-primary btn-sm btn-flat" ng-disabled="btnSave==0">
                                            Save
                                        </button>
                                        &nbsp;

                                        <button type="button" ng-click="saveCandidates('n')" class="btn btn-default btn-sm btn-flat" ng-disabled="btnSave==0">
                                            Save and New
                                        </button>
                                        &nbsp;
                                        @*<button type="button" ng-click="clear()" class="btn btn-danger btn-sm" ng-disabled="btnSave==0">
                                                <i class="fa fa-remove"></i>
                                                Clear
                                            </button>*@
                                        <button class="btn btn-danger btn-sm btn-flat" ng-disabled="btnSave==0" ng-click="cancel()">
                                            <i class="fa fa-remove"></i>
                                            Cancel
                                        </button>
                                    </div>
                                </div>
                                <!-- /.card-header -->
                                <!-- form start -->

                                <div>
                                    <p style="font-weight:bold; padding-left:10px; font-size:20px;"> Basic Info </p>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Email</span>
                                            <i style="color:red;">*</i>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control validate[required,'Please Enter Email'] Txtbox" id="em" ng-model="c.email" maxlength="50" />
                                        </div>
                                        <div class="col-md-2">
                                            <span>First Name</span>
                                            <i style="color:red;">*</i>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control validate[required,'Please Enter First Name'] Txtbox" id="fn" ng-model="c.firstName" maxlength="50" />


                                        </div>

                                    </div>

                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Middle Name</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.middleName" maxlength="50" />
                                        </div>

                                        <div class="col-md-2">
                                            <span>Last Name</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.lastName" maxlength="50" />

                                        </div>
                                    </div>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Phone</span>
                                         </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.phone" maxlength="18"  numbers-only/>
                                        </div>
                                        <div class="col-md-2">
                                            <span>Mobile</span>
                                            <i style="color:red;">*</i>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control validate[required,''] Txtbox" ng-model="c.mobile" maxlength="10" numbers-only id="mob" />
                                        </div>
                                    </div>

                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Secondary Email</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.secondaryEmail" maxlength="50" />
                                        </div>
                                    </div>

                                    <p style="font-weight:bold; padding-left:10px; font-size:20px;"> Address Information </p>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Street</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" id="odt" ng-model="c.street" maxlength="100" />
                                        </div>
                                        <div class="col-md-2">
                                            <span>Zip/Postal Code</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" id="odt" ng-model="c.postalCode" maxlength="50" />
                                        </div>
                                    </div>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>City</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.city" maxlength="50" />
                                        </div>
                                        <div class="col-md-2">
                                            <span>State/Province</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.state" maxlength="50" />
                                        </div>
                                    </div>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Country</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.country" maxlength="50" />
                                        </div>

                                    </div>

                                    <p style="font-weight:bold; padding-left:10px; font-size:20px;"> Professional Details </p>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Experience in Years</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.experienceInYear" maxlength="50" />
                                        </div>
                                        <div class="col-md-2">
                                            <span>Highest Qualification</span>
                                        </div>
                                        <div class="col-md-4">
                                            <select class="form-control" ng-model="c.quaId" ng-options="q.qualId as q.qualification   for q in listQualification">
                                                <option value="Select">Select</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Current Job Title</span>
                                        </div>
                                        <div class="col-md-4">
                                            <select class="form-control" ng-model="c.currentJobTitle">
                                                <option value="None">None</option>
                                                <option value="Fresher">Fresher</option>
                                                <option value="Project-Lead">Project-Lead</option>
                                                <option value="Project-Manager">Project-Manager</option>
                                            </select>
                                        </div>
                                        <div class="col-md-2">
                                            <span>Current Employer</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.currentEmployer" maxlength="50" />
                                        </div>
                                    </div>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Expected Salary</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.expectedSalary" maxlength="12" valid-number />
                                        </div>
                                        <div class="col-md-2">
                                            <span>Current Salary</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.currentSalary" maxlength="12" valid-number />
                                        </div>
                                    </div>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span>Skill Set</span>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" ng-model="c.skillSet" maxlength="100" />
                                        </div>

                                    </div>
                                    <p style="font-weight:bold; padding-left:10px; font-size:20px;">Other Info </p>
                                    <div class="form-group form-inline" style="margin-top:20px;">

                                        <div class="col-md-2">
                                            <span>Candidate Status</span>
                                            <i style="color:red;">*</i>
                                        </div>
                                        <div class="col-md-4">
                                            <select class="form-control  validate[required,'Please Select Candidate Status']" ng-model="c.csSubCatId" ng-options="c.csSubCatId as c.csSubCat group by c.csCat for c in listCSCat_SubCat" id="csts"></select>
                                        </div>
                                        <div class="col-md-2">
                                            <span>Source</span>
                                            <i style="color:red;">*</i>
                                        </div>
                                        <div class="col-md-4">
                                            <select ng-options="s.sourceId as s.source for s in listSource" ng-model="c.sourceId" class="form-control  validate[required,'Please Select Source']" id="src"></select>
                                        </div>
                                    </div>
                                    @*<div class="form-group form-inline" style="margin-top:20px;">

                                        <div class="col-md-2">
                                            <span>Candidate Owner</span>
                                            <i style="color:red;">*</i>
                                        </div>
                                        <div class="col-md-4">
                                            <select class="form-control  validate[required,'Please Select Candidate Owner']" id="cdo" ng-model="c.recruiterId" ng-options="r.recruiterId as r.recruiterName for r in listRecruiter"></select>
                                        </div>

                                    </div>*@

                                    <p style="font-weight:bold; padding-left:10px; font-size:20px;">
                                        Educational Details &nbsp;&nbsp;&nbsp;
                                        <a style="color:cornflowerblue; cursor:pointer; font-weight:500; font-size:17px;" ng-click="addInListEduDetails()">+Add Educational Details</a>
                                    </p>

                                    <div ng-repeat="ed in listEduDetail">
                                        <div class="form-group form-inline" style="margin-top:10px;">
                                            <div class="col-md-12">
                                                <button type="button" class="btn btn-danger btn-sm" ng-click="deleteEducationDetails($index,ed.eduId)" ng-disabled="btnSave==0" style="float:right;padding: 4px;
    padding-top: 1px;
    padding-bottom: 1px !important;">
                                                    <i class="fa fa-remove"></i>
                                                </button>
                                            </div>



                                        </div>
                                        <div class="form-group form-inline" style="margin-top:20px;">

                                            <div class="col-md-2">
                                                <span>Institute/School</span>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" ng-model="ed.institute" maxlength="100" />
                                            </div>
                                            <div class="col-md-2">
                                                <span>Major Department</span>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" ng-model="ed.departmant" maxlength="100" />
                                            </div>
                                        </div>
                                        <div class="form-group form-inline" style="margin-top:20px;">

                                            <div class="col-md-2">
                                                <span>Degree</span>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" ng-model="ed.degree" maxlength="100" />
                                            </div>
                                            <div class="col-md-4">
                                                <span>Currently Pursuing <input type="checkbox" ng-model="ed.currentlyPursuing" /> </span>
                                            </div>

                                        </div>
                                        <div class="form-group form-inline" style="margin-top:20px;">
                                            <div class="col-md-2">
                                                <span>Duration</span>
                                            </div>
                                            <div class="col-md-1">
                                                <span>From</span>
                                            </div>
                                            <div class="col-md-1">
                                                <select ng-options="m for m in listMonth" ng-model="ed.fromMonth" class="form-control"></select>
                                            </div>
                                            <div class="col-md-1">
                                                <select ng-options="y for y in listYear" ng-model="ed.fromYear" class="form-control"></select>
                                            </div>
                                            <div class="col-md-1">

                                            </div>


                                            <div class="col-md-1" ng-hide="ed.currentlyPursuing==true">
                                                <span>To</span>
                                            </div>
                                            <div class="col-md-1" ng-hide="ed.currentlyPursuing==true">
                                                <select ng-options="m for m in listMonth" ng-model="ed.toMonth" class="form-control"></select>
                                            </div>
                                            <div class="col-md-1" ng-hide="ed.currentlyPursuing==true">
                                                <select ng-options="y for y in listYear" ng-model="ed.toYear" class="form-control"></select>
                                            </div>


                                        </div>
                                        @*<div class="form-group form-inline" style="margin-top:20px;">
                                                <div class="col-md-2">
                                                    <button type="button" class="btn btn-danger btn-sm" ng-click="deleteEducationDetails($index,ed.eduId)" ng-disabled="btnSave==0"><i class="fa fa-trash"></i>  Delete</button>
                                                </div>



                                            </div>*@
                                        <hr />
                                    </div>




                                    <p style="font-weight:bold; padding-left:10px; font-size:20px;">
                                        Experience Details &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        <a style="color:cornflowerblue; cursor:pointer; font-weight:500; font-size:17px;" ng-click="addInListExpDetails()">+Add Experience Details</a>
                                    </p>

                                    <div ng-repeat="ex in listExpDetail">
                                        <div class="form-group form-inline" style="margin-top:10px;">
                                            <div class="col-md-12">
                                                <button type="button" class="btn btn-danger btn-sm" ng-click="deleteExperienceDetails($index,ed.eduId)" ng-disabled="btnSave==0" style="float:right;padding: 4px;
    padding-top: 1px;
    padding-bottom: 1px !important;">
                                                    <i class="fa fa-remove"></i>

                                                </button>
                                            </div>

                                        </div>
                                        <div class="form-group form-inline" style="margin-top:20px;">

                                            <div class="col-md-2">
                                                <span>Occupation / Title</span>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" ng-model="ex.occupation" maxlength="100" />
                                            </div>
                                            <div class="col-md-2">
                                                <span>Company</span>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" ng-model="ex.company" maxlength="100" />
                                            </div>
                                        </div>

                                        <div class="form-group form-inline" style="margin-top:20px;">

                                            <div class="col-md-2">
                                                <span>Summery</span>
                                            </div>
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" ng-model="ex.summery" maxlength="100" />
                                            </div>
                                            <div class="col-md-4">
                                                <span>I Currently work here <input type="checkbox" ng-model="ex.currentlyWork" /> </span>
                                            </div>

                                        </div>
                                        <div class="form-group form-inline" style="margin-top:20px;">
                                            <div class="col-md-2">
                                                <span>Duration</span>
                                            </div>
                                            <div class="col-md-1">
                                                <span>From</span>
                                            </div>
                                            <div class="col-md-1">
                                                <select ng-options="m for m in listMonth" ng-model="ex.fromMonth" class="form-control"></select>
                                            </div>
                                            <div class="col-md-1">
                                                <select ng-options="y for y in listYear" ng-model="ex.fromYear" class="form-control"></select>
                                            </div>
                                            <div class="col-md-1">

                                            </div>


                                            <div class="col-md-1" ng-hide="ex.currentlyWork==true">
                                                <span>To</span>
                                            </div>
                                            <div class="col-md-1" ng-hide="ex.currentlyWork==true">
                                                <select ng-options="m for m in listMonth" ng-model="ex.toMonth" class="form-control"></select>
                                            </div>
                                            <div class="col-md-1" ng-hide="ex.currentlyWork==true">
                                                <select ng-options="y for y in listYear" ng-model="ex.toYear" class="form-control"></select>
                                            </div>


                                        </div>
                                        @*<div class="form-group form-inline" style="margin-top:20px;">
                                                <div class="col-md-2">
                                                    <button type="button" class="btn btn-danger btn-sm" ng-click="deleteExperienceDetails($index,ed.eduId)" ng-disabled="btnSave==0">
                                                        <i class="fa fa-trash"></i>
                                                        Delete
                                                    </button>
                                                </div>

                                            </div>*@

                                        <hr />

                                    </div>
                                    <p style="font-weight:bold; padding-left:10px; font-size:20px;">Attachment Information </p>
                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span style="padding-left:15px;">Resume</span>
                                        </div>

                                        <div class="col-md-2">
                                            <input type="file" id="resume" accept=".pdf,.doc"/>
                                        </div>
                                        <div class="col-md-1">

                                        </div>
                                        <div class="col-md-4">
                                            <a href="~/Files/{{c.resume}}" ng-show="c.resume!=null && c.resume!=''" download> View Resume</a>
                                        </div>

                                    </div>

                                    <div class="form-group form-inline" style="margin-top:20px;">
                                        <div class="col-md-2">
                                            <span style="padding-left:15px;">Others</span>
                                        </div>

                                        <div class="col-md-2">
                                            <input type="file" id="others" accept=".pdf,.doc"/>
                                        </div>
                                        <div class="col-md-1">

                                        </div>
                                        <div class="col-md-4">
                                            <a href="~/Files/{{c.others}}" ng-show="c.others!=null && c.others!=''" download>View Other Document</a>
                                        </div>

                                    </div>
                                    <!-- /.card-body -->
                                    @*<div class="card-footer" align="center" style="margin-top:40px;">
                                            &nbsp;&nbsp;
                                            &nbsp;&nbsp;&nbsp;&nbsp;
                                            <button type="button" ng-click="saveJobOpeningInfo()" class="btn btn-success btn-sm">
                                                <i class="fa fa-save"></i>
                                                Save
                                            </button>
                                            &nbsp;&nbsp;&nbsp;&nbsp;
                                            <button type="button" ng-click="clear()" class="btn btn-danger btn-sm">
                                                <i class="fa fa-remove"></i>
                                                Clear
                                            </button>
                                        </div>*@
                                </div>
                                <div class="card-header bg-gray-light">
                                    <div style="float:right; margin-top:1px;">

                                        <button type="button" ng-click="saveCandidates()" class="btn btn-primary btn-sm btn-flat" ng-disabled="btnSave==0">
                                            Save
                                        </button>
                                        &nbsp;

                                        <button type="button" ng-click="saveCandidates('n')" class="btn btn-default btn-sm btn-flat" ng-disabled="btnSave==0">
                                            Save and New
                                        </button>
                                        &nbsp;
                                        @*<button type="button" ng-click="clear()" class="btn btn-danger btn-sm" ng-disabled="btnSave==0">
                                                <i class="fa fa-remove"></i>
                                                Clear
                                            </button>*@
                                        <button class="btn btn-danger btn-sm btn-flat" ng-disabled="btnSave==0" ng-click="cancel()">
                                            <i class="fa fa-remove"></i>
                                            Cancel
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card ng-cloak" ng-show="divCandidate==1">
                    <div class="card-header bg-gray-light">
                        <div class="btn-group">
                            <h5 class="mb-0" style="font-weight:bold;">Candidates </h5>
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div style="height:4px;margin-top:-4px;">
                                <button type="button" class="btn btn-light btn-sm btn-flat" ng-click="editCandidates()" ng-disabled="btnEdit==0">Edit</button>
                            </div>
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <div style="height:4px;margin-top:-4px;">
                                <button type="button" class="btn btn-danger btn-sm btn-flat" ng-disabled="btnDelete==0" ng-click="deleteCandidates()">
                                    <i class="fa fa-trash"></i>
                                    Delete
                                </button>
                            </div>
                            &nbsp;&nbsp;&nbsp;&nbsp;
                            <div style="height:4px;margin-top:-4px;">
                                <div class="dropdown" ng-hide="btnDelete==0" >
                                    <button class="btn btn-flat" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="background-color:white; height:32px; border-color:lightgrey;">
                                  <i class="fa fa-ellipsis-h"></i>    
                                    </button>
                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                        <a class="dropdown-item" ng-click="sendCandidatesIds_To_associateJobOpening()" style="cursor:pointer;">Associate Job Openings</a>
                                        
                                    </div>
                                </div>
                                </div>
                            &nbsp;&nbsp;&nbsp;&nbsp;
                            <div style="height:4px;margin-top:-4px;">
                                <div class="dropdown" ng-hide="btnDelete==0">
                                    <button class="btn  dropdown-toggle btn-sm" type="button" id="actionButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="color:darkgreen; background-color:white; border:1px solid;">
                                        Actions
                                    </button>
                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                        <a class="dropdown-item" href="#" ng-click="sendCandidatesIds_To_sendEmail()">Send Email</a>

                                    </div>
                                </div>
                            </div>
                            </div>
                        <div style="float:right; margin-right:40px; height:30px;">
                            <button type="button" class="btn btn-default btn-sm btn-flat" ng-click="new()" ng-show="btnNew==1">
                                <i class="fa fa-plus"></i>
                                New
                            </button>

                        </div>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body table-responsive">
                        <table  class="dataTable table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th><input type="checkbox" ng-model="chkboxHeader" ng-change="allChkboxChecked_UnChecked()"
                                               style="cursor:pointer;" /> </th>
                                    <th>Candidate Name</th>
                                    @*<th>Current Job Title</th>*@
                                    <th>Contact No</th>
                                    <th>Email Id</th>
                                    <th>Candidate Status</th>
                                    @*<th>Candidate Owner</th>*@
                                    <th>CV</th>
                                  <th>Applied Jobs</th>  
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="cd in listCandidates" ng-show="listCandidates.length>0">
                                    <td><input type="checkbox" ng-model="cd.chkbox" ng-change="checkEditDelete()" style="cursor:pointer;" /> </td>
                                    <td> <a href='/Candidates/showCandidateInfo?candidateId={{cd.cId}}' class="btn btn-link btn-sm">{{cd.firstName}}</a>    </td>
                                    @*<td>{{cd.currentJobTitle}}</td>*@
                                    <td>{{cd.mobile}}</td>
                                    <td>{{cd.email}}</td>
                                    <td>{{cd.csSubCat}}
                                    <span ng-show="cd.positionTile!=null && cd.positionTile!='' && cd.positionTile!=undefined">for&nbsp;  <strong> {{cd.positionTile}}</strong></span>
                                    </td>
                                    @*<td>{{cd.recruiterName}}</td>*@
                                    <td>
                                    <a href="~/Files/{{cd.resume}}" ng-show="cd.resume!=null && cd.resume!=''" download> View Resume</a>
                                        <span style="padding-left:0px !important;" ng-hide="cd.resume!=null && cd.resume!=''">Not Found</span>
                                    </td>
                                   <td>
                                       <button type="button" class="btn btn-sm btn-link" ng-click="showModalAssociateJob_To_Candidate(cd.cId)">{{cd.noOfAppliedJobs}}</button>
                                   </td>

                                </tr>
                            </tbody>
                            <tfoot>
                            </tfoot>
                        </table>
                    </div>
                    <div ng-hide="listCandidates.length>0">
                        <p style="text-align:center;">
                            No Record Available.

